{
  "compilerOptions": {
    "target": "ES2022",
    "incremental": true,
    "noUncheckedIndexedAccess": true,
    "strict": true,
    "isolatedModules": true,
		"esModuleInterop": true,
		"jsx": "react-jsx",
		"module": "ESNext",
		"moduleResolution": "Bundler",
  },
  "include": ["api/**/*.ts", "api/**/*.tsx"]
}
